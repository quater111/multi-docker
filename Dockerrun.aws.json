{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
    {
        "name": "client",
        "image": "bachelorprojectsoti/multi-client",
        "hostname": "client",
        "essential": false
    },
    {
        "name": "server",
        "image": "bachelorprojectsoti/multi-server",
        "hostname": "api",
        "essential": false
    },
    {
        "name": "worker",
        "image": "bachelorprojectsoti/multi-worker",
        "hostname": "worker",
        "essential": false
    },
    {
        "name": "nginx",
        "image": "bachelorprojectsoti/multi-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
            {
            "hostPort": 80,
            "containerPort": 80
            }
        ],
        "links": ["client", "server"]
    }  
    ]
}